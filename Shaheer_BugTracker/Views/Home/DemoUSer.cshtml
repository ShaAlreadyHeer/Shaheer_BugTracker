</head>


<!-- Header top area start-->
<div class="header-top-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="admin-logo">
                    <a href="#">
                        <img src="~/Images/log.png" alt="" />
                    </a>
                </div>
            </div>
            <div class="col-lg-5 col-md-5 col-sm-0 col-xs-12">
                <div class="header-top-menu">
                    <ul class="nav navbar-nav mai-top-nav">
                        <li class="nav-item">
                        </li>
                        <li class="nav-item">
                        </li>
                        <li class="nav-item">
                        </li>
                        <li class="nav-item dropdown">
                        </li>
                        <li class="nav-item">
                        </li>
                    </ul>
                </div>
            </div>
            <div class="col-lg-4 col-md-9 col-sm-6 col-xs-12">
                <div class="header-right-info">
                    <br />
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Header top area end-->

<section>
    <!-- Breadcome start-->
    <div class="breadcome-area mg-t-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcome-list shadow-reset">
                        <div class="breadcome-heading text-center">
                            <h1>Demo Login</h1>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Breadcome End-->
    <!-- login Start-->
    <div class="row">
        <div class="login-form-area mg-t-30 mg-b-40">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3">
                        @using (Html.BeginForm("DemoLoginAsync", "Account", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
                            @Html.Hidden("emailKey", "DemoAdminEmail")

                            <section class="panel">
                                <header class="panel-heading container-fluid">
                                    <div class="text-center">
                                        <img src="~/Avatars/face_co.png" />

                                    </div>
                                    <div class="panel-heading-icon bg-primary mt-sm text-center">
                                        Admin
                                    </div>
                                    <hr />
                                    <div class="row container-fluid text-center">
                                        Ability to create Projects and Tickets and accessing forbidden areas.
                                        Assign/Unassign users to Roles, Projects, and Tickets
                                    </div>
                                </header>
                                <div class="panel-body text-center">
                                    <button type="submit" class="btn btn-xs btn-primary">Login</button>
                                </div>
                            </section>
                        }
                    </div>
                    <div class="col-lg-3">
                        @using (Html.BeginForm("DemoLoginAsync", "Account", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
                            @Html.Hidden("emailKey", "DemoPMEmail")

                            <section class="panel">
                                <header class="panel-heading container-fluid">
                                    <div class="text-center">
                                        <img src="~/Avatars/face_co (1).png" />
                                    </div>
                                    <div class="panel-heading-icon bg-primary mt-sm text-center">
                                        Project Manager
                                    </div>
                                    <hr />
                                    <div class="row container-fluid text-center">
                                        Create Projects, Edit given Projects and Tickets.
                                        Assign/Unassign Developers from Projects and Tickets
                                    </div>
                                </header>
                                <div class="panel-body text-center">
                                    <button type="submit" class="btn btn-xs btn-primary">Login</button>
                                </div>
                            </section>
                        }
                    </div>
                    <div class="col-lg-3">
                        @using (Html.BeginForm("DemoLoginAsync", "Account", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
                            @Html.Hidden("emailKey", "DemoDeveloperEmail")

                            <section class="panel">
                                <header class="panel-heading">
                                    <div class="text-center">
                                        <img src="~/Avatars/face_co (4).png" />

                                    </div>
                                    <div class="panel-heading-icon bg-primary mt-sm text-center">
                                        Developer
                                    </div>
                                    <hr />
                                    <div class="row container-fluid text-center">
                                        View Projects and Tickets which you are assigned to. Create update comments on Tickets.
                                        Assigned to work on Projects and Tickets.
                                    </div>
                                </header>
                                <div class="panel-body text-center">
                                    <button type="submit" class="btn btn-xs btn-primary">Login</button>
                                </div>
                            </section>
                        }
                    </div>
                    <div class="col-lg-3">
                        @using (Html.BeginForm("DemoLoginAsync", "Account", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
                            @Html.Hidden("emailKey", "DemoSubmitterEmail")

                            <section class="panel">
                                <header class="panel-heading">
                                    <div class="text-center">
                                        <img src="~/Avatars/face_co (3).png" />

                                    </div>
                                    <div class="panel-heading-icon bg-primary mt-sm text-center">
                                        Submitter
                                    </div>
                                    <hr />
                                    <div class="row text-center container-fluid">
                                        Create Tickets for assigned Projects.
                                        Edit and Comment on tickets which you have submitted.
                                    </div>
                                </header>
                                <div class="panel-body text-center">
                                    <button type="submit" class="btn btn-xs btn-primary">Login</button>
                                </div>
                            </section>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- login End-->
    <!-- Footer Start-->
    <div class="footer-copyright-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="footer-copy-right">
                        <p>Copyright &#169; 2018 Colorlib All rights reserved. Template by <a href="https://colorlib.com">Colorlib</a>.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Footer End-->
    <script src="~/Scripts/jquery-1.11.3.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.meanmenu.js"></script>
    <script src="~/Scripts/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Scripts/jquery.sticky.js"></script>
    <script src="~/Scripts/jquery.scrollUp.min.js"></script>
    <script src="~/Scripts/jquery.form.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/form-active.js"></script>
    <script src="~/Scripts/main.js"></script>

</section>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

}

