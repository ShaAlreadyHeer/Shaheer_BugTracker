                            <section class="panel">
                                <header class="panel-heading">
                                    <div class="panel-heading-icon bg-primary mt-sm">
                                        <i class="fa fa-bitbucket">Project Manager</i>
                                    </div>
                                </header>
                                <div class="panel-body">
                                    <button type="submit" class="btn btn-xs btn-primary">Login</button>
                                </div>
                            </section>
                        }
                    </div>
                    <div class="col-lg-3">
                        @using (Html.BeginForm("DemoLoginAsync", "Account", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
