<section>
    <!-- Breadcome start-->
    <div class="breadcome-area mg-t-40">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcome-list shadow-reset">
                        <div class="breadcome-heading text-center">
                            <h1>Demo Login</h1>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Breadcome End-->
    <!-- login Start-->
    <div class="row">
        <div class="login-form-area mg-t-30 mg-b-40">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3">
                        @using (Html.BeginForm("DemoLoginAsync", "Account", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
                            @Html.Hidden("emailKey","DemoAdminEmail")

                            <section class="panel">
                                <header class="panel-heading">
                                    <div class="panel-heading-icon bg-primary mt-sm">
                                       <i class="fa fa-bitbucket">Admin</i> 
                                    </div>
                                </header>
                                <div class="panel-body">
                                    <button type="submit" class="btn btn-xs btn-primary">Login</button>
                                </div>
                            </section>
                        }
                    </div>
                    <div class="col-lg-3">
                        @using (Html.BeginForm("DemoLoginAsync", "Account", FormMethod.Post))
                        {
                            @Html.AntiForgeryToken()
